{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "headers": [
    {
      "source": "/link-haven-contact.vcf",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/vcard"
        }
      ]
    },
    {
      "source": "/(.*\\.(js|css|svg|png|jpg|jpeg|gif|woff2?|ttf|eot|ico|json))",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    }
  ],
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
