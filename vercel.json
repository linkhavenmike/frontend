{
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*\\.(js|css|svg|png|jpg|jpeg|gif|woff2?|ttf|eot|ico|json))",
        "headers": { "cache-control": "public, max-age=31536000, immutable" }
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  